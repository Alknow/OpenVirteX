<configuration status="DEBUG">
  <appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{1} - %msg%n"/>
    </Console>
  </appenders>
  <loggers>
    <root level="info" includeLocation="false">
      <appender-ref ref="Console"/>
    </root>
  </loggers>
</configuration>
